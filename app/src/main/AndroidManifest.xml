<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lily.YumYum">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.lily.YumYum.FoodApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.lily.YumYum.ui.foodlist.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lily.YumYum.ui.details.DetailActivity" />
        <activity android:name="com.lily.YumYum.ui.ownrecipe.OwnRecipeActivity" />
        <activity android:name="com.lily.YumYum.ui.ownrecipe.AddFoodActivity" />
    </application>

</manifest>